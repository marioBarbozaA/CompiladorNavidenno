            }
          // fall through
          case 88: break;
          case 30:
            { return new Symbol(sym.ENTREGA, yytext());
            }
          // fall through
          case 89: break;
          case 31:
            { string.append("\"");
            }
          // fall through
          case 90: break;
          case 32:
            { string.append("\n");
            }
          // fall through
          case 91: break;
          case 33:
            { string.append("\r");
            }
          // fall through
          case 92: break;
          case 34:
            { string.append("\t");
            }
          // fall through
          case 93: break;
          case 35:
            { return new Symbol(sym.HACE_DO, "do");
            }
          // fall through
          case 94: break;
